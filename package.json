{
  "name": "natuin",
  "description": "Natuin is an alternative to Atuin API server written in typescript",
  "version": "0.0.0",
  "type": "commonjs",
  "private": true,
  "scripts": {
    "pretest": "node scripts/version-update.mjs",
    "build": "tsc -p tsconfig.build-prod.json",
    "clean": "tsc --build --clean tsconfig.build-prod.json",
    "env2doc": "npm run -s build && env2doc -d -p dist > ENVIRONMENT.md",
    "env2doc:check": "env2doc -d -p dist > .env2doc-check.tmp && diff .env2doc-check.tmp ENVIRONMENT.md",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "prebuild": "node scripts/version-update.mjs",
    "preversion": "npm run lint && npm run test && npm run env2doc:check",
    "start": "node dist/otel.js",
    "start:dev": "TZ=utc nodemon src/otel.ts",
    "test": "jest -c jest.config.mjs"
  },
  "dependencies": {
    "@fluidware-it/express-microservice": "0.5.0",
    "@fluidware-it/express-mysql2-middleware": "0.2.0",
    "@fluidware-it/healthz-server": "0.1.0",
    "@fluidware-it/httpclient": "0.3.0-rc.0",
    "@fluidware-it/mysql2-client": "0.6.1",
    "@fluidware-it/saddlebag": "0.1.1",
    "@opentelemetry/api": "1.9.0",
    "@opentelemetry/instrumentation-grpc": "0.57.0",
    "@opentelemetry/instrumentation-http": "0.57.0",
    "@opentelemetry/instrumentation-mysql2": "0.45.0",
    "@opentelemetry/instrumentation-pino": "0.46.0",
    "@opentelemetry/instrumentation-undici": "0.10.0",
    "@opentelemetry/resources": "1.30.0",
    "@opentelemetry/sdk-node": "0.57.0",
    "@scalar/express-api-reference": "0.2.68",
    "dotenv": "16.4.5",
    "express": "4.21.2",
    "mysql2": "3.11.4",
    "nodemailer": "6.9.12",
    "pino": "8.19.0",
    "prefixed-api-key": "1.1.1",
    "pug": "3.0.3",
    "semver": "7.6.3",
    "ulid": "2.3.0"
  },
  "devDependencies": {
    "@eslint/js": "9.18.0",
    "@fluidware-it/env2doc": "0.3.1",
    "@testcontainers/mariadb": "10.16.0",
    "@types/express": "5.0.0",
    "@types/jest": "29.5.14",
    "@types/nodemailer": "6.4.17",
    "@types/pug": "2.0.10",
    "@types/semver": "7.5.8",
    "@typescript-eslint/eslint-plugin": "8.19.1",
    "@typescript-eslint/parser": "8.19.1",
    "eslint": "9.18.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-n": "17.15.1",
    "eslint-plugin-prettier": "5.2.1",
    "globals": "14.0.0",
    "jest": "29.7.0",
    "nodemon": "3.1.9",
    "prettier": "3.4.2",
    "testcontainers": "10.16.0",
    "ts-jest": "29.2.5",
    "ts-node": "10.9.2",
    "typescript": "5.7.3",
    "typescript-eslint": "8.19.1"
  },
  "engineStrict": true,
  "engines": {
    "node": "22.13.1"
  },
  "nodemonConfig": {
    "delay": 2000,
    "verbose": true,
    "execMap": {
      "ts": "ts-node"
    },
    "signal": "SIGTERM",
    "ext": "js,ts,json,yml,yaml",
    "watch": [
      "src",
      "openapi",
      ".env"
    ]
  }
}
